{
  "name": "hello-kitty-theme",
  "displayName": "Hello Kitty Theme",
  "description": "Cute Hello Kitty themed VSCode theme - Full of pink tones and kawaii colors!",
  "version": "1.4.0",
  "main": "./extension.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "hello kitty",
    "pink",
    "cute",
    "kawaii",
    "girly",
    "pastel",
    "soft",
    "kawaii"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Hello Kitty",
        "uiTheme": "vs-dark",
        "path": "./themes/hello-kitty-color-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "hello-kitty-icons",
        "label": "Hello Kitty Icons",
        "path": "./themes/hello-kitty-icon-theme.json"
      }
    ],
    "configuration": {
      "title": "Hello Kitty Theme",
      "properties": {
        "editor.fontFamily": {
          "type": "string",
          "default": "'Comic Sans MS', 'Comic Sans', 'Segoe UI', 'Arial', sans-serif",
          "description": "Hello Kitty theme recommended font"
        },
        "editor.fontSize": {
          "type": "number",
          "default": 14,
          "description": "Hello Kitty theme recommended font size"
        },
        "terminal.fontFamily": {
          "type": "string",
          "default": "'Comic Sans MS', 'Comic Sans', 'Monaco', 'Menlo', 'Ubuntu Mono', monospace",
          "description": "Hello Kitty theme recommended terminal font"
        }
      }
    },
    "viewsWelcome": [
      {
        "view": "explorer",
        "contents": "üéÄ Welcome to Hello Kitty Theme! \n\nIf you love this theme, please consider giving us a ‚≠ê star on GitHub!\n\n[‚≠ê Star Repository](command:helloKittyTheme.starRepo)\n[üé® View Gallery](command:helloKittyTheme.viewGallery)\n[üìñ Documentation](command:helloKittyTheme.openDocs)"
      }
    ],
    "commands": [
      {
        "command": "helloKittyTheme.starRepo",
        "title": "‚≠ê Star Hello Kitty Theme Repository",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.viewGallery",
        "title": "üé® View Theme Gallery",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.openDocs",
        "title": "üìñ Open Documentation",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.showWelcome",
        "title": "üéÄ Show Welcome Message",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.toggleFont",
        "title": "üéÄ Toggle Hello Kitty Font",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.showSnippets",
        "title": "üéÄ Show Hello Kitty Snippets",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.toggleIntensity",
        "title": "üéÄ Toggle Theme Intensity",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.showGitSnippets",
        "title": "üéÄ Show Hello Kitty Git Snippets",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.showDebugInfo",
        "title": "üéÄ Show Hello Kitty Debug Features",
        "category": "Hello Kitty Theme"
      },
      {
        "command": "helloKittyTheme.showFileIcons",
        "title": "üéÄ Show Hello Kitty File Icons",
        "category": "Hello Kitty Theme"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "helloKittyTheme.starRepo",
          "when": "false"
        },
        {
          "command": "helloKittyTheme.viewGallery",
          "when": "false"
        },
        {
          "command": "helloKittyTheme.openDocs",
          "when": "false"
        },
        {
          "command": "helloKittyTheme.showWelcome",
          "when": "false"
        }
      ]
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "git-commit",
        "path": "./snippets/git.json"
      },
      {
        "language": "git-rebase",
        "path": "./snippets/git.json"
      }
    ]
  },
  "publisher": "HelloKittyVSCodeTheme",
  "repository": {
    "type": "git",
    "url": "https://github.com/oguzhan18/hello-kitty-vscode-theme"
  },
  "author": {
    "name": "Oƒüuzhan √áART",
    "email": "oguzhancart1@gmail.com"
  },
  "license": "MIT",
  "bugs": {
      "url": "https://github.com/oguzhan18/hello-kitty-vscode-theme/issues"
  },
  "homepage": "https://github.com/oguzhan18/hello-kitty-vscode-theme#readme",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FF69B4",
    "theme": "dark"
  }
}
 