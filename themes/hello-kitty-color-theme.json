  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#E57373",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.quoted.single", "string.quoted.double"],
      "settings": {
        "foreground": "#388E3C"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C2185B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#D84315"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#F57C00"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#7B1FA2"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#C2185B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C2185B"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#388E3C"
      }
    },
    {
      "name": "JavaScript Objects",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#D84315"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#C2185B"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#C2185B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Git Diff Added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#388E3C"
      }
    },
    {
      "name": "Git Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#D32F2F"
      }
    }
  ]
} 